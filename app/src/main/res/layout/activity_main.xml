<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/ToSignUpPagebtn"
        android:layout_width="148dp"
        android:layout_height="71dp"
        android:layout_marginTop="5dp"
        android:text="SignUp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.524"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Homescreen" />

    <Button
        android:id="@+id/Homescreen"
        android:layout_width="148dp"
        android:layout_height="71dp"
        android:layout_marginTop="16dp"
        android:text="SignUp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.524"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/loggedinstatus"
        android:layout_width="313dp"
        android:layout_height="45dp"
        android:layout_marginTop="24dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ToChat" />

    <Button
        android:id="@+id/ToSignInPagebtn"
        android:layout_width="148dp"
        android:layout_height="71dp"
        android:layout_marginTop="10dp"
        android:text="SignIn"
        app:layout_constraintStart_toStartOf="@+id/ToSignUpPagebtn"
        app:layout_constraintTop_toBottomOf="@+id/ToSignUpPagebtn" />

    <Button
        android:id="@+id/LogoutBtb"
        android:layout_width="148dp"
        android:layout_height="71dp"
        android:layout_marginTop="12dp"
        android:text="Log Out"
        app:layout_constraintStart_toStartOf="@+id/ToSignInPagebtn"
        app:layout_constraintTop_toBottomOf="@+id/ToSignInPagebtn" />

    <Button
        android:id="@+id/Mapsbtn"
        android:layout_width="148dp"
        android:layout_height="71dp"
        android:layout_marginTop="10dp"
        android:text="ToMaps"
        app:layout_constraintStart_toStartOf="@+id/LogoutBtb"
        app:layout_constraintTop_toBottomOf="@+id/LogoutBtb" />

    <Button
        android:id="@+id/btncard"
        android:layout_width="148dp"
        android:layout_height="71dp"
        android:layout_marginTop="10dp"
        android:text="cards"
        app:layout_constraintStart_toStartOf="@+id/Mapsbtn"
        app:layout_constraintTop_toBottomOf="@+id/Mapsbtn" />

    <Button
        android:id="@+id/ToChat"
        android:layout_width="148dp"
        android:layout_height="71dp"
        android:layout_marginTop="10dp"
        android:text="Chat"
        app:layout_constraintStart_toStartOf="@+id/btncard"
        app:layout_constraintTop_toBottomOf="@+id/btncard" />

</androidx.constraintlayout.widget.ConstraintLayout>